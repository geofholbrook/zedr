{
  "name": "zedr",
  "version": "0.0.0",
  "description": "easy CRUD routing with plugin support",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest --forceExit",
    "format": "echo 'no formatter implemented'"
  },
  "author": "Geof Holbrook",
  "license": "ISC",
  "dependencies": {
    "express": "^4.19.2",
    "lodash": "^4.17.21",
    "typescript": "^5.5.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.7",
    "@types/node": "^20.14.11",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "pre-commit": "^1.2.2",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": ["**/?(*.)+(spec|test).ts"]
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "none",
    "bracketSpacing": true,
    "tabWidth": 4,
    "arrowParens": "avoid"
  },
  "pre-commit": [
    "build",
    "test"
  ]
